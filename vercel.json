{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "public",
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/transcript",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ],
  "public": true
}
